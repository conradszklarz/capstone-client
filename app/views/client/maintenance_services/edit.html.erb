<% if @errors %>
  <ul>
    <% @errors.each do |error| %>
      <li><%= error %></li>
    <% end %>
  </ul>
<% end %>

<%= form_tag "/client/maintenance_services/#{@maintenance_service["id"]}", method: :patch do %>
  <div>
    Service Id: <input type="text" name="service_id" value="<%= @maintenance_service["service_id"] %>" />
  </div>
  <div>
    Maintenance Event ID: <input type="text" name="maintenance_event_id" value="<%= @maintenance_service["maintenance_event"]["id"] %>">
  </div>
  <input type="submit" name="" value="Update Maintenance Service"/>
<% end %>