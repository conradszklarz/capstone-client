<% if @errors %>
  <ul>
    <% @errors.each do |error| %>
      <li><%= error %></li>
    <% end %>
  </ul>
<% end %>

<%= form_tag "/client/car_parts/#{@car_part["id"]}", method: :patch do %>
  <div>
    Part Number: <input type="text" name="part_number" value="<%= @car_part["part_number"] %>" />
  </div>
  <div>
    Price: <input type="text" name="price" value="<%= @car_part["price"] %>" />
  </div>
  <div>
    Date: <input type="text" name="date" value="<%= @car_part["date"] %>" />
  </div>
  <div>
    Maintenance Event ID: <input type="text" name="maintenance_event_id" value="<%= @car_part["maintenance_event"]["id"] %>">
  </div>
  <input type="submit" name="" value="Update Car Part"/>
<% end %>
